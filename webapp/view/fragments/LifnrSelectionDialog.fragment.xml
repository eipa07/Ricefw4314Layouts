<core:FragmentDefinition
    id="lifnrFragment"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <SelectDialog
        id="lifnrSeldial1"
        title="{i18n>fragments.lifnr.title}"
        class="sapUiPopupWithPadding"
        items="{MP_LayoutModelService>/ZZ1_CDS_SEARCH_HELP_LIFNR}"
        search="onVHSearchLifnr"
        confirm="onVHCloseLifnr"
        cancel="onVHCloseLifnr"
    >
        <StandardListItem
            id="LifnrStdList"
            iconDensityAware="false"
            iconInset="false"
            title="{MP_LayoutModelService>Lifnr}"
            description="{MP_LayoutModelService>LifnrName}"
        />
    </SelectDialog>

    <!--
   
    -->
</core:FragmentDefinition>
